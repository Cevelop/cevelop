<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.cevelop.codeanalysator.feature"
      label="Codeanalysator"
      version="1.14.2.qualifier"
      provider-name="IFS Institute for Software"
      plugin="com.cevelop.branding"
      license-feature="com.cevelop.license.feature"
      license-feature-version="1.14.2.qualifier">

   <description url="https://www.cevelop.com/features">
      The Codeanalysator is an Eclipse CDT plug-in that provides hints
and quickfixes to enhance code quality by adhering to the MISRA,
Autosar and C++ Core Guidelines.
   </description>

   <copyright>
      Copyright (c) 2019 IFS Institute for Software
Copyright (c) 2021 The Authors of Cevelop
This program and the accompanying materials are made available
under the terms of the Eclipse Public License 2.0 which accompanies
this distribution, and is available at https://www.eclipse.org/legal/epl-2.0/
SPDX-License-Identifier: EPL-2.0
   </copyright>

   <license>
      %license%
   </license>

   <url>
      <update label="Cevelop Plug-Ins" url="https://www.cevelop.com/update/latest"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime" version="3.12.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.cdt.core" version="6.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.cdt.codan.checkers" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.cdt.codan.core" version="4.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.cdt.codan.core.cxx" version="3.4.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.cdt.codan.ui" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.cdt.codan.ui.cxx" version="3.3.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface.text" version="3.11.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ltk.core.refactoring" version="3.7.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.ide" version="3.12.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.equinox.registry"/>
      <import plugin="org.eclipse.cdt.ui"/>
      <import plugin="org.eclipse.swt"/>
      <import plugin="org.eclipse.ltk.ui.refactoring" version="3.8.0" match="greaterOrEqual"/>
      <import plugin="ch.hsr.ifs.iltis.cpp.core"/>
      <import plugin="ch.hsr.ifs.iltis.core"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.commands"/>
   </requires>

   <plugin
         id="com.cevelop.codeanalysator.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.cevelop.codeanalysator.autosar"
         download-size="0"
         install-size="0"
         version="0.0.0"
         fragment="true"
         unpack="false"/>

   <plugin
         id="com.cevelop.codeanalysator.cppcore"
         download-size="0"
         install-size="0"
         version="0.0.0"
         fragment="true"
         unpack="false"/>

   <plugin
         id="com.cevelop.codeanalysator.help"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.cevelop.codeanalysator.misra"
         download-size="0"
         install-size="0"
         version="0.0.0"
         fragment="true"
         unpack="false"/>

</feature>
