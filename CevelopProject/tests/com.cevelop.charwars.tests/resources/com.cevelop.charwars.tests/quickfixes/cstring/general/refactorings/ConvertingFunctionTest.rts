//!AtofStd
//@main.cpp
#include <cstdlib>

int main(){
	char s[] = "0.01";
	double n = std::atof(s);
}
//=
#include <cstdlib>
#include <string>

int main(){
	std::string s = "0.01";
	double n = stod(s);
}


