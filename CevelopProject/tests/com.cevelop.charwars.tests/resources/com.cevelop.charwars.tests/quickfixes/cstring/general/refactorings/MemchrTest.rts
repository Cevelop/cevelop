//@main.cpp
#include <cstring>
#include "iostream.h"

int main(){
	char *url = "http://www.google.com";
	if(memchr(url, '-', strlen(url))) {
		std::cout << url;
	}
	while(memchr(url, '-', strlen(url))) {
		std::cout << url;
	}
	for(; memchr(url, '-', strlen(url));) {
		std::cout << url;
	}
	char* ptr = memchr(url, '-', strlen(url)) ? memchr(url, '-', strlen(url)) : url;
	char* x = memchr(url, '-', strlen(url));
	x = memchr(url, '-', strlen(url));
	bool y = memchr(url, '-', strlen(url));
	y = memchr(url, '-', strlen(url));
	do {
		std::cout << url;
	} while(memchr(url, '-', strlen(url)));
	
	int index = memchr(url, '-', strlen(url)) - url;
}
