#include <cstring>
#include <string>
#include <algorithm>
#include "iostream.h"

int main(){
	std::string url = "http://www.google.com";
	if (find(url.begin(), url.end(), '-') != url.end()) {
		std::cout << url;
	}
	while (find(url.begin(), url.end(), '-') != url.end()) {
		std::cout << url;
	}
	for (; find(url.begin(), url.end(), '-') != url.end();) {
		std::cout << url;
	}
	char* ptr =
			find(url.begin(), url.end(), '-') != url.end() ?
					memchr(url.c_str(), '-', url.size()) : url.c_str();
	char* x = memchr(url.c_str(), '-', url.size());
	x = memchr(url.c_str(), '-', url.size());
	bool y = find(url.begin(), url.end(), '-') != url.end();
	y = find(url.begin(), url.end(), '-') != url.end();
	do {
		std::cout << url;
	} while (find(url.begin(), url.end(), '-') != url.end());
	
	int index = memchr(url.c_str(), '-', url.size()) - url.c_str();
}


